  "scripts": {
    "preinstall": "npx only-allow yarn",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "i18n-clean": "rimraf -rf dist/i18n/i18n.generated.ts",
    "start:local": "cross-env NODE_ENV=local nest start --watch",
    "local": "yarn start:local",
    "l": "yarn local",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "dev": "yarn start:dev",
    "start:staging": "cross-env NODE_ENV=staging nest start --watch",
    "stg": "yarn start:staging",
    "start:debug": "cross-env NODE_ENV=local nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typeorm": "ts-node --require tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:run": "yarn typeorm migration:run -- -d ./db/typeorm.config.ts",
    "db:restart:e2e": "docker-compose down db-e2e && docker-compose up db-e2e -d && sleep 2",
    "test:e2e": "cross-env NODE_ENV=e2e jest -i --no-cache --watchAll --runInBand --detectOpenHandles --forceExit --config ./test/e2e/jest-e2e.ts"
  },
  "engines": {
    "npm": "Please use yarn"
  },
